	month = sep,
	year = {2004},
	keywords = {Biochemistry, denitrification, fertilizer, fossil fuel combustion, general, Geochemistry, Haber-Bosch, nitrogen, nitrogen fixation, Soil Science \& Conservation, Terrestrial Pollution},
	pages = {153--226}
