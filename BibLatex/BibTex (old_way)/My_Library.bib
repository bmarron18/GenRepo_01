	pages = {386--393},
	file = {IEEE Xplore Abstract Record:/home/bmarron/.mozilla/firefox/vmro2bq5.default/zotero/storage/9JU6ETTI/abs_all.html:text/html;IEEE Xplore Full Text PDF:/home/bmarron/.mozilla/firefox/vmro2bq5.default/zotero/storage/2WTSXBPX/Sumner et al. - 2012 - Predicting Dark Triad Personality Traits from Twit.pdf:application/pdf}
