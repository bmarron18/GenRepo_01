@article{barnosky_approaching_2012,
	title = {Approaching a state shift in {Earth}/'s biosphere},
	volume = {486},
	copyright = {© 2012 Nature Publishing Group, a division of Macmillan Publishers Limited. All Rights Reserved.},
	issn = {0028-0836},
