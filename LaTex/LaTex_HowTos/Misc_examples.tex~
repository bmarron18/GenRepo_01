% ----summations -------------------------------------
\begin{align*}
  A &= \sum_{n=-\infty}^{+\infty} f(x) \\
%  B &= \smashoperator[r]{\sum_{n=-\infty}^{+\infty}} f(x) \\
%  C &= \sum_{\mathclap{n=-\infty}}^{+\infty} f(x) \\
  D &= \sum_{\substack{n={}\\-\infty}}^{+\infty} f(x) \\
  E &= \sum_{-\infty}^{\infty}\mathop{}_{\mkern-5mu n} f(x)
\end{align*}


